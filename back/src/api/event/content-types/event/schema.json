{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.image",
        "content.text"
      ],
      "required": true
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "address": {
      "type": "string"
    },
    "distance": {
      "type": "text"
    }
  }
}
